@using Undersoft.SDK.Service.Application.GUI.View.Accounts
@inherits GenericDialog<TModel>

@typeparam TModel where TModel : class, IOrigin, IInnerProxy, IAuthorization

@implements IDialogContentComponent<IViewData<TModel>>
@implements IView

<CascadingValue Value="@Content">
    
    <GenericFormWizard TValidator="AccountValidator<TModel>" TModel="TModel" Position="StepperPosition.Left" Content="@Content" />

</CascadingValue>

@code
{
    protected override void OnInitialized()
    {
        Content.View = this;
    }
}
